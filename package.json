{
  "name": "i18nizer",
  "description": "CLI to extract texts from JSX/TSX and generate i18n JSON with AI translations",
  "version": "0.7.0",
  "author": "Yoannis Sanchez Soto",
  "bin": "./bin/run.js",
  "bugs": "https://github.com/yossTheDev/i18nizer/issues",
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@huggingface/inference": "^4.13.5",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "chalk": "^5.6.2",
    "inquirer": "^13.1.0",
    "js-yaml": "^4.1.1",
    "node-fetch": "^3.3.2",
    "openai": "^6.15.0",
    "ora": "^9.0.0",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.9",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/yossTheDev/i18nizer#readme",
  "keywords": [
    "oclif",
    "i18n",
    "cli",
    "translation",
    "ai",
    "generative-ai",
    "google"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "i18nizer",
    "dirname": "i18nizer",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "yossTheDev/i18nizer",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "packageManager": "yarn@4.12.0"
}
